[code]
	pager = cat

[url "https://git-codecommit.us-east-1.amazonaws.com/v1/repos"]
	insteadof = mhp:

[credential "mhp:"]
	UseHttpPath = true
	helper = !aws --profile CodeCommitProfile codecommit credential-helper $@
	username = ghoward

