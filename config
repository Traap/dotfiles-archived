# Access to HostName when disconnected from Amgen network.
Host stash
    HostName stash.amgencss.fpcomplete.com
    Port 2222
    User gahoward
    IdentityFile ~/.ssh/css_rsa

# This alias seems to be used when git access submodules from stash.
Host stash.amgencss.fpcomplete.com
    Port 2222
    User gahoward
    IdentityFile ~/.ssh/css_rsa

# Access to HostNmae when connected to Amgen network.
Host s2 
