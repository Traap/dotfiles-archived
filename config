# contents of $HOME/.ssh/config
Host *
    ServerAliveInterval 300
    IdentityFile ~/.ssh/css_rsa
    User gahoward
    
# GitHub settings
Host github.com
    HostName github.com
    User Traap
    IdentityFile ~/.ssh/traap_rsa

# AWS: fpc 
Host fpc 
    HostName ec2-52-8-13-116.us-west-1.compute.amazonaws.com
    Port 22

# Access to HMST git server when disconnected from Amgen network.
Host stash
    HostName stash.amgencss.fpcomplete.com
    Port 2222

# This alias is to be used when git access submodules from stash.
Host stash.amgencss.fpcomplete.com
    Port 2222

# Access to HMST git server when connected to Amgen network.
Host s2 
    HostName 52.8.106.215
    Port 7999 

# Access to my personal Amazom Linux machines
Host moe
    HostName ec2-54-201-140-13.us-west-2.compute.amazonaws.com
    Port 22
    User ubuntu
    IdentityFile ~/.ssh/moe_rsa

# Access to FH Score
Host fhs
    HostName gitlab-dal.devops.amgen.com
    Port 2222
    IdentityFile ~/.ssh/fhscore_rsa
